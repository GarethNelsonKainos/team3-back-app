{
	"name": "my-project",
	"version": "1.0.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"test": "vitest",
		"test:ui": "vitest --ui",
		"coverage": "vitest --coverage",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"dev": "tsx watch src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"ci:generate": "dotenv -e .env.test -- prisma generate",
		"ci:build": "dotenv -e .env.test -- npm run build",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate dev",
		"db:studio": "prisma studio",
		"db:seed": "prisma db seed"
	},
	"prisma": {
		"seed": "tsx src/seed.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"@types/express": "^5.0.6",
		"@types/node": "^24.10.12",
		"@types/pg": "^8.16.0",
		"@types/supertest": "^6.0.2",
		"@vitest/coverage-v8": "^4.0.18",
		"dotenv-cli": "^8.0.0",
		"prisma": "^7.3.0",
		"supertest": "^7.0.0",
		"tsup": "^8.0.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^6.19.2",
		"@prisma/client": "^7.3.0",
		"dotenv": "^17.2.4",
		"express": "^5.2.1"
	}
}
